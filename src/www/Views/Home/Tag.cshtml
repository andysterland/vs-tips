@model TagViewModel

<div class="row center-block">
    <div class="jumbotron text-center">
        <div class="container">
            <div class="col-md-2">
                <a href="/">
                    <img src="~/images/tag-vs-random.png" />
                    <div>Random Tips</div>
                </a>
            </div>
            <div class="col-md-2  @(@Model.GetViewName() == "tag/editor"  ? "nav-active" : "")">
                <a href="/tag/editor">
                    <img src="~/images/tag-vs-edit.png" />
                    <div>Editor Tips</div>
                </a>
            </div>
            <div class="col-md-2 @(@Model.GetViewName() == "tag/shell"  ? "nav-active" : "")">
                <a href="/tag/shell">
                    <img src="~/images/tag-vs-shell.png" />
                    <div>Shell Tips</div>
                </a>
            </div>
            <div class="col-md-2 @(@Model.GetViewName() == "tag/debug"  ? "nav-active" : "")">
                <a href="/tag/debugger">
                    <img src="~/images/tag-vs-debug.png" />
                    <div>Debugging Tips</div>
                </a>
            </div>
            <div class="col-md-2 @(@Model.GetViewName() == "tag/design"  ? "nav-active" : "")">
                <a href="/tag/desginer">
                    <img src="~/images/tag-vs-design.png" />
                    <div>Designer Tips</div>
                </a>
            </div>
            <div class="col-md-2">
                <a href="/search">
                    <img src="~/images/tag-vs-search.png" />
                    <div>Search Tips</div>
                </a>
            </div>
        </div>
    </div>
</div>

<div class="row center-block">
</div>

<div class="row center-block">
    <h2>@Model.GetPageTitle()</h2>
</div>

<div class="row center-block">
    @Html.Partial("_TipsTablePartial", Model.GetTipsViewModel())
</div>
